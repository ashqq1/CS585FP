import junit.framework.Test;
import junit.framework.TestSuite;

public class CS585Project {

  public static Test suite() {
    TestSuite suite = new TestSuite();
    suite.addTestSuite(yahoo.class);
    suite.addTestSuite(blackboardSignIn.class);
    suite.addTestSuite(save file.class);
    suite.addTestSuite(runscript alert.class);
    suite.addTestSuite(confirm.class);
    suite.addTestSuite(verifying google title and pic logo.class);
    return suite;
  }

  public static void main(String[] args) {
    junit.textui.TestRunner.run(suite());
  }
}
